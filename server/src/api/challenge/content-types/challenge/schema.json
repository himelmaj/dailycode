{
  "kind": "collectionType",
  "collectionName": "challenges",
  "info": {
    "singularName": "challenge",
    "pluralName": "challenges",
    "displayName": "Challenge",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "problem": {
      "type": "richtext",
      "required": true
    },
    "code": {
      "type": "text",
      "required": true
    },
    "solution": {
      "type": "text",
      "required": true
    },
    "levels": {
      "type": "enumeration",
      "enum": [
        "easy",
        "medium",
        "hard",
        "expert"
      ],
      "required": true,
      "default": "medium"
    },
    "lang": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lang.lang",
      "inversedBy": "challenges"
    }
  }
}
